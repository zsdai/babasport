<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itheima.core.dao.TestDao">

	<insert id="insertTest" parameterType="TestDB">
		insert into t_test
		(id,name,birthday)
		values
		(#{id},#{name},#{birthday})
	</insert>
</mapper>
